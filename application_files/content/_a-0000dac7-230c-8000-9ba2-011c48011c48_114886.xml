            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#&quot;_a-0000dae1-7535-8000-9ba2-011c48011c48_122821&quot;(todate(caladddays(ri!bidSubmissionDate,1)),time(23,59,00))+4/24</definition>
