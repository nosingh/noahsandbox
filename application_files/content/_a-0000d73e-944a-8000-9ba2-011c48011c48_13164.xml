<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>APN_uiPagingGridAutoColumns</name>
        <uuid>_a-0000d73e-944a-8000-9ba2-011c48011c48_13164</uuid>
        <description>Returns a Grid Component with paging enabled and the Columns automatically derived from the DataSubset (required) and parallel arrays of column labels (required), fields (required), alignments (optional), and formatter rules (optional). 'pagingInfo' (required) is a local variable of type PagingInfo established with load().</description>
        <parentUuid>_a-0000d73e-9720-8000-9ba2-011c48011c48_59983</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#&quot;SYSTEM_SYSRULES_gridField&quot;(
  label: ri!label,
  instructions: ri!instructions,
  columns: apply(
    #&quot;_a-0000d73e-944a-8000-9ba2-011c48011c48_13169&quot;(label: _, field: _, data: ri!dataSubset.data),
    merge(
      ri!columnLabels,
      ri!columnFields
    )
  ),
  value: ri!pagingInfo,
  saveInto: ri!pagingInfo,
  totalCount: ri!dataSubset.totalCount
)</definition>
        <namedTypedValue>
            <name>label</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>instructions</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>dataSubset</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>pagingInfo</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>columnLabels</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>columnFields</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role allowForAll="false" inherit="true" name="readers">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="true" name="authors">
            <users/>
