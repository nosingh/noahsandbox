<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>APN_getRandomInteger</name>
        <uuid>_a-0000d62a-67b3-8000-da6f-010000010000_943</uuid>
        <description>Gets an integer [1,max]</description>
        <parentUuid>_a-0000d62a-67b3-8000-da6f-010000010000_958</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=floor(rand()*ri!maxint)+1</definition>
        <namedTypedValue>
            <name>maxint</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
