<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>APN_firstDayOfCurrentYear</name>
        <uuid>_a-0000d7d3-de4c-8000-4594-010000010000_2301</uuid>
        <description>Returns the first day of the current calendar year</description>
        <parentUuid>_a-0000d62a-67b3-8000-da6f-010000010000_956</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=eomonth(today(),-month(today()))+1</definition>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
